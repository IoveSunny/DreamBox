	menuconfig PACKAGE_kmod-b43
		tristate "kmod-b43.................................. Broadcom 43xx wireless support"
		default y if DEFAULT_kmod-b43
		default m if ALL
		depends (!(TARGET_avr32||TARGET_ep93xx)||BROKEN)
		depends !(!(TARGET_brcm47xx||TARGET_brcm63xx)) || !TARGET_brcm63xx
		depends !(!(TARGET_brcm47xx||TARGET_brcm63xx)) || !TARGET_brcm4716
		select PACKAGE_kmod-mac80211
		depends !(!(TARGET_brcm47xx||TARGET_brcm63xx)) || !TARGET_brcm47xx
